@model EdgeRealEstate.Entities.EmloyeeTarget
@{
    ViewBag.Title = "Delete EmloyeeTarget";
    Layout = "~/Views/Shared/_AdminPage.cshtml";
}
<br />
<h3 style="text-align:center">هدف الموظف</h3>
<h4 dir="rtl">هل أنت متأكد من أنك تريد حذف هذا؟</h4>
<div class="form-horizontal" dir="rtl">
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.id, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("الكود", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.Employee.ARName, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("الموظف", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.typeOfOperation, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("نوع العملية", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.typeOfCommission, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("نوع الخصم", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.precent, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("نسبة الخصم", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.fromMoney, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("من مال", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.toMoney, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("الى مال", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="حذف" class="btn btn-primary" /> |
            @Html.ActionLink("رجوع", "Index")
        </div>
    }
</div>


