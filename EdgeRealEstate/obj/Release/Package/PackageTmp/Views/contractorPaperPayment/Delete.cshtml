@model EdgeRealEstate.Entities.contractorPaperPayment
@{
    ViewBag.Title = "Delete PaperPayment";
    Layout = "~/Views/Shared/_AdminPage.cshtml";
}
<br />
<h3 style="text-align:center">سند صرف نقدية مقاول</h3>
<h4 dir="rtl">هل أنت متأكد من أنك تريد حذف هذا السند؟</h4>
<div class="form-horizontal" dir="rtl">
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.id, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("الكود", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.contractor.ARName, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("العميل - المورد", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.Employee.ARName, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("الموظف مدخل البيانات", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.Employee1.ARName, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("المندوب", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.paidMethod, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("طريقة الدفع", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.paid, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("المدفوع", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.refType, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("نوع المرجع", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.indate, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("فى تاريخ", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.notes, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("ملاحظات", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="حذف" class="btn btn-primary" /> |
            @Html.ActionLink("رجوع", "Index")
        </div>
    }
</div>


