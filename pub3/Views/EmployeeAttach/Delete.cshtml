@model EdgeRealEstate.Entities.EmployeeAttach
@{
    ViewBag.Title = "Delete EmployeeAttach";
    Layout = "~/Views/Shared/_AdminPage.cshtml";
}
<br />
<h3 style="text-align:center">ملفات الموظف</h3>
<h4 dir="rtl">هل أنت متأكد من أنك تريد حذف هذا الملف ؟</h4>
<div class="form-horizontal" dir="rtl">
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.id, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("الكود", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.Employee.ARName, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("الموظف", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.FileName, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        @Html.Label("اسم الملف", htmlAttributes: new { @class = "control-label col-md-2" })
    </div>
    @using (Html.BeginForm("delete", "EmployeeAttach", FormMethod.Post,
                            new { enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="حذف" class="btn btn-primary" /> |
            @Html.ActionLink("رجوع", "Index")
        </div>
    }
</div>


