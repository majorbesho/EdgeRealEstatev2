            @Html.DisplayFor(modelItem => item.Flat.Aname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CurrntlyDate)
